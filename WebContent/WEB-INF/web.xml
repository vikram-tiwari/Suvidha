<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Suvidha</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>login</servlet-name>
  	<servlet-class>Login</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>addNewAdmin</servlet-name>
  	<servlet-class>AddNewAdmin</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>displayVisitors</servlet-name>
  	<servlet-class>DisplayVisitors</servlet-class>
  </servlet>
   <servlet>
  	<servlet-name>displayProprietors</servlet-name>
  	<servlet-class>DisplayProprietor</servlet-class>
  </servlet>
   <servlet>
  	<servlet-name>showAdmins</servlet-name>
  	<servlet-class>ShowAdmin</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>placeInformation</servlet-name>
  	<servlet-class>PlaceInformation</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>proprietorPlaces</servlet-name>
  	<servlet-class>ProprietorPlaces</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>proprietorPlaces</servlet-name>
  	<url-pattern>/proprietorPlaces</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>placeInformation</servlet-name>
  	<url-pattern>/placeInformation</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>showAdmins</servlet-name>
  	<url-pattern>/showAdmin</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
  	<servlet-name>displayProprietors</servlet-name>
  	<url-pattern>/displayProprietor</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>displayVisitors</servlet-name>
  	<url-pattern>/displayVisitors</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>addNewAdmin</servlet-name>
  	<url-pattern>/AddNewAdmin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
</web-app>